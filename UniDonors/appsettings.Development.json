{
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://localhost:9200",
          "indexFormat": "unidonors-dev-{0:yyyy.MM.dd}"
        } 
      }
    ],
    "Enrich": [ "FromLogContext" ]
  }
}